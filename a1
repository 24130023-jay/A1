#include <iostream.h>
#include <conio.h>
void main() {
    clrscr();   // Clear screen
    int num = 10;         // Integer variable
    float decimal = 5.75; // Float variable
    char letter = 'A';    // Character variable
    cout << "Integer value: " << num << endl;
    cout << "Float value: " << decimal << endl;
    cout << "Character value: " << letter << endl;
    getch();   
}
